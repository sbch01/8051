ORG	0000H
	SJMP START
;ORG 	000BH	;ADDRES OF TMRO INTERUPT VECTOR
;	SJMP TMR0_INTR
;ORG	001BH
;	SJMP TMR1_INTR
;ORG	0003H
;	SJMP INT0_INTR
;ORG	0013H
;	SJMP INT1_INTR
;ORG	0023H
;	SJMP UART_INT

 MYBYTE EQU 20H	

ORG	0030H
START: 	MOV A,#0FH
	MOV R0,#02H
	MOV R1,#03H
	MOV R2,#04H
	MOV R3,#05H
	MOV R4,#06H
	MOV R5,#07H
	MOV R6,#08H
	MOV R7,#09H

	MOV MYBYTE,#12H

	PUSH MYBYTE
	PUSH 04H
	MOV A,#11H
	PUSH 20H
	MOV A,#00H
	POP A
	POP A
	
	NOP	;LOOP HERE
	AJMP $-1
	END
