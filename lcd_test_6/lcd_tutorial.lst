                 =1    162     ;Routine to send command to LCD
                 =1    163
                 =1    164     LCD_command:
0099 F5A0        =1    165              mov   LCD_data,A     ;Move the command to LCD port
009B C290        =1    166              clr   LCD_rs         ;Selected command register
009D C291        =1    167              clr   LCD_rw         ;We are writing in instruction register
009F D292        =1    168              setb  LCD_en         ;Enable H->
                 =1    169
00A1 C292        =1    170              clr   LCD_en
00A3 11CB        =1    171              acall LCD_busy       ;Wait for LCD to process the command
00A5 22          =1    172              ret                  ;Return from busy routine
