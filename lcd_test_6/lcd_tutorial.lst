                 =2    252     ; Subroutine wait mS
                 =2    253     ;----------------------------------------------------------------------
                 =2    254     ;Load A and B register with your delay needs
                 =2    255     ;A register is low byte digit and B register is high byte register
                 =2    256     ;Always check value in two register before call subroutine
                 =2    257     ;Max delay is 65535 mS if A=0xff and B=0xff
                 =2    258     Wait_mS:
00E2 05F0        =2    259             INC B
                 =2    260     Loop_mS:
00E4 78F9        =2    261             MOV R0,#249
00E6 D8FE        =2    262             DJNZ R0,$
00E8 78F9        =2    263             MOV R0,#249
00EA D8FE        =2    264             DJNZ R0,$
00EC D5E0F5      =2    265             DJNZ A,Loop_mS
00EF D5F0F2      =2    266             DJNZ B,Loop_mS
00F2 22          =2    267             RET
