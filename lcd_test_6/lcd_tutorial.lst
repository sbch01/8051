                 =1    179     LCD_sendchar:
00A6 F5A0        =1    180              mov   LCD_data,A     ;Move the command to LCD port
00A8 D290        =1    181              setb  LCD_rs         ;Selected data register
00AA C291        =1    182              clr   LCD_rw         ;We are writing
00AC D292        =1    183              setb  LCD_en         ;Enable H->
                 =1    184
00AE C292        =1    185              clr   LCD_en
00B0 11CB        =1    186              acall LCD_busy       ;Wait for LCD to process the data
00B2 22          =1    187              ret                  ;Return from busy routine
